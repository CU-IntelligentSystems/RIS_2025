<?xml version="1.0"?>
<package format="2">

  <name>Localization</name>

  <version>0.1.0</version>

  <description>Configuration, launch files, and converter node for Duckiebot localization using robot_localization.</description>

  <maintainer email="alex.v.tranca@gmail.com">Alexandru Tranca</maintainer>

  <license>MIT</license>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>robot_localization</depend>

  <depend>nav_msgs</depend>       
  <depend>sensor_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>std_msgs</depend>         
  <depend>tf2_ros</depend>          
  <depend>tf</depend>               
  <depend>rospy</depend>            
  <depend>duckietown_msgs</depend>  
  <depend>python3-numpy</depend>
  </package>